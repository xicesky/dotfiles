
!!! stuff !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!Xcursor*theme: Vanilla-DMZ-AA
Xcursor*theme:  DMZ-White
Xcursor.size:   12

!Xft.dpi:        96
!Xft.antialias:  false
Xft.antialias:  true
!Xft.rgba:       rgb
!Xft.hinting:    false
Xft.hinting:    true
!Xft.hintstyle:  hintslight
Xft.hintstyle:  hintfull

!!! urxvt !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
! General
!urxvt*termName:  rxvt-256color
urxvt*loginShell:        false
urxvt*scrollBar:         false
urxvt*secondaryScroll:    true
urxvt*saveLines:         65535
urxvt*cursorBlink:       false
urxvt*urgentOnBell:       true
urxvt*visualBell:         true
!urxvt*override-redirect:false
!urxvt*borderLess:       false
!urxvt*internalBorder:       0
!urxvt*externalBorder:       0
! Disable that pesky "keycap picture insert mode"
urxvt.iso14755_52:       false

! Extensions
urxvt*perl-lib:             /usr/lib/urxvt/perl/
urxvt*perl-ext-common:      default,matcher,searchable-scrollback,clipboard
urxvt*urlLauncher:          /usr/bin/xdg-open
urxvt*url-launcher:         /usr/bin/xdg-open
urxvt*matcher.button:       1
urxvt.keysym.M-c:           perl:clipboard:copy
urxvt.keysym.M-v:           perl:clipboard:paste
urxvt.keysym.M-C-v:         perl:clipboard:paste_escaped
urxvt.clipboard.autocopy:   true
urxvt.clipboard.copycmd:    xsel -ib
urxvt.clipboard.pastecmd:   xsel -ob

!  - catch ugly URLs
!urxvt.cutchars:        `()'*<>[]{|}"    '
!urxvt.cutchars:         ()*<>[]{|}

! Appearance
urxvt.iconFile:     /usr/share/icons/Mint-X/apps/scalable/Terminal.svg
!  - use a bitmap font
!urxvt*font:        -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
!urxvt*boldFont:    -xos4-terminus-bold-*-*-*-12-*-*-*-*-*-*-*
!  - use xft for drawing fonts
!urxvt*font:       xft:Terminus:8

! You can append options e.g. :autohint=true:pixelsize=12
urxvt*font:             xft:Bitstream Vera Sans Mono:pixelsize=12
urxvt*boldFont:         xft:Bitstream Vera Sans Mono:bold
urxvt*italicFont:       xft:Bitstream Vera Sans Mono:italic
urxvt*boldItalicFont:   xft:Bitstream Vera Sans Mono:bold:italic
! rxvt should underline this URL: https://www.q1cc.net/

!  - cursor
urxvt*cursorColor: #DCDCDC
!  - pseudo transparency
!urxvt*shading:    50
!urxvt*transparent:true

!!  - color scheme
!urxvt.background:  #000000
!urxvt.foreground:  #f0f0f0
!! black + red
!!urxvt*color0:     #3f3f3f
!urxvt*color0:      #000000
!urxvt*color1:      #e01010
!! green + yellow
!urxvt*color2:      #10e010
!urxvt*color3:      #e0e010
!! blue + purple
!urxvt*color4:      #1010e0
!urxvt*color5:      #e010e0
!! cyan + white
!urxvt*color6:      #1050e0
!urxvt*color7:      #e0e0e0
!! bright-black + bright-red
!urxvt*color8:      #6a6a6a
!urxvt*color9:      #ff5555
!! bright-green + bright-yellow
!urxvt*color10:     #55ff55
!urxvt*color11:     #ffff55
!! bright-blue + bright-purple
!urxvt*color12:     #5555ff
!urxvt*color13:     #e655ff
!! bright-cyan + bright-white
!urxvt*color14:     #55d5ff
!urxvt*color15:     #ffffff

